<?xml version="1.0" encoding="UTF-8"?>
<web-app 
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	
    <welcome-file-list>
        <welcome-file>accueil</welcome-file>
    </welcome-file-list>
    
    <servlet>
        <servlet-name>WelcomePage</servlet-name>
        <servlet-class>controllers.servlet.WelcomePage</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>ButtonHeader</servlet-name>
        <servlet-class>controllers.servlet.ButtonHeader</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Parier</servlet-name>
        <servlet-class>controllers.servlet.Parier</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Inscrire</servlet-name>
        <servlet-class>controllers.servlet.Inscrire</servlet-class>
    </servlet>
    
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>controllers.servlet.Login</servlet-class>
    </servlet>
</web-app>